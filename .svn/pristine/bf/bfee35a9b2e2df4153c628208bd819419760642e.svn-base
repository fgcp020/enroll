.table_pager{float:left;width:100%;text-align:center;margin-top:15px;position: relative;height: 55px;}
.table_pager .form_group{display:inline-block;width:auto;float:none}
.table_pager .form_group .btn{float:left;border-width:1px 0 1px 1px;border-radius:0}
.table_pager .form_group .btn:last-child{border-width:1px 1px 1px 1px}
.table_pager .form_group .form_control{float:left;margin-right:10px;text-align:center}
.table_pager .table_pager_num{position: absolute;left: 0;top:10px}
.table_pager .layui-btn-group .layui-btn{border-left: none;min-width: 45px;text-align: center}
.table_pager .layui-btn-group .layui-btn:first-child{border-left:1px solid #c9c9c9 !important}
.table_control .un-check tr.sel td{background: #009688;color: #fff}
.public_table_pager_total{
    right: 0;
    position: absolute;
    top:10px;

}
/*.table_control tr:nth-child(2n){background: #f8f8f8}*/
.table_control td > span{display: block;word-break: break-all;overflow: hidden;max-width: 300px;text-overflow: ellipsis}
.table_control td > span.sortFiled{white-space: nowrap}
.table_control td span a{color: #009688;text-decoration: underline;white-space: nowrap;cursor: pointer}
.hide_1 th:nth-child(1),.hide_1 td:nth-child(1){display: none;}
