.table_control th,.table_control td{min-width:100px}
.layui-table th,.layui-table td{min-width:100px; border-color: rgb(57, 154, 203);  padding: 5px 15px;}
.table_control th{font-weight:normal;cursor: pointer;}
.layui-table th{font-weight:500;cursor: pointer;}
.layui-table thead tr { background-color: #CEE5F2 !important ;font-weight: 500;}
.layui-table tbody tr.marked {color: #00a0e9}
