<div id="declarationDegree" xmlns="http://www.w3.org/1999/html">
    <div class="audit_content" style="min-width: 340px;">
        <!--基本信息-->
        <label class="title col-xs-12" style="font-size: 24px;text-align: center;margin-bottom: 20px;">{{pageTitle}}</label>
        <div class="border_left border_right border_top" style="float: left;width: 100%">
            <div class="form_row border_bottom">
                <strong class="form_label title col-xs-12">学生个人基础信息</strong>
            </div>
            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;学生姓名:</label>
                <input class="form_control col-xs-3 min_wit" ng-model="registerMsg.user.username" type="text" ng-blur="checkStuCode()"/>
            </div>
            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;性别:</label>
                <!-- <select class="form_control col-xs-3" ng-model="selRow.student_sexs">
					  <option value ="male">男</option>
					  <option value ="female">女</option>
					</select> -->
				<div class="layui-unselect col-xs-3 layui-form-select min_wit">
                    <div class="layui-select-title">
                        <input type="text" class="layui-input layui-unselect" readonly="readonly" value=""
                               placeholder="" ng-model="registerMsg.user.sex_text"/>
                        <i class="layui-edge"></i>
                    </div>
                    <dl class="layui-anim layui-anim-upbit">
                        <dd ng-repeat="item in allSex"
                            ng-click="registerMsg.user.sex = item.value;registerMsg.user.sex_text = item.label"
                            ng-class="{'layui-this':registerMsg.user.sex_text == item.label}">{{item.label}}
                        </dd>
                    </dl>
                </div>
            </div>
            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;手机号:</label>
                <input class="form_control col-xs-3 min_wit" ng-model="registerMsg.user.phone" maxlength="11" type="text" placeholder="学生或父母监护人手机号" ng-change="checkPhone(1)"/>
                <em style="color: red;line-height: 40px;margin-left: 15px;display: inline-flex;" ng-if="phone1Invalid">手机号格式有误</em>
                <em style="color: red;line-height: 40px;margin-left: 15px;display: inline-flex;" ng-if="isStu">用于接收验证码和消息通知，请确保正确填写。</em>
            </div>
            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;籍贯:</label>
                <input class="form_control col-xs-3 min_wit" ng-model="registerMsg.userExtendedInfo.nativePlace" type="text"/>
            </div>
            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;民族:</label>
                <div class="layui-unselect col-xs-3 layui-form-select min_wit">
                    <div class="layui-select-title">
                        <input type="text" class="layui-input layui-unselect" readonly="readonly" value=""
                               placeholder="" ng-model="registerMsg.userExtendedInfo.nation_text"/>
                        <i class="layui-edge"></i>
                    </div>
                    <dl class="layui-anim layui-anim-upbit">
                        <dd ng-repeat="item in allNation"
                            ng-click="registerMsg.userExtendedInfo.nation = item.id;registerMsg.userExtendedInfo.nation_text = item.content"
                            ng-class="{'layui-this':registerMsg.userExtendedInfo.nation_text == item.content}">{{item.content}}
                        </dd>
                    </dl>
                </div>
            </div>
            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;港澳台侨外:</label>
                <div class="layui-unselect col-xs-3 layui-form-select  min_wit">
                    <div class="layui-select-title">
                        <input type="text" class="layui-input layui-unselect" readonly="readonly" value=""
                               placeholder="" ng-model="registerMsg.userExtendedInfo.noMainland_text"/>
                        <i class="layui-edge"></i>
                    </div>
                    <dl class="layui-anim layui-anim-upbit">
                        <dd ng-repeat="item in toChoose"
                            ng-click="registerMsg.userExtendedInfo.noMainland_text = item.label;registerMsg.userExtendedInfo.noMainland = item.value"
                            ng-class="{'layui-this':registerMsg.userExtendedInfo.noMainland_text == item.label}">{{item.label}}
                        </dd>
                    </dl>
                </div>
            </div>
            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;国籍/地区:</label>
                <div class="layui-unselect col-xs-3 layui-form-select  min_wit">
                    <div class="layui-select-title">
                        <input type="text" class="layui-input layui-unselect" readonly="readonly" value=""
                               placeholder="" ng-model="registerMsg.userExtendedInfo.country_text"/>
                        <i class="layui-edge"></i>
                    </div>
                    <dl class="layui-anim layui-anim-upbit">
                        <dd ng-repeat="item in allCountry"
                            ng-click="registerMsg.userExtendedInfo.country = item.id;registerMsg.userExtendedInfo.country_text = item.content;"
                            ng-class="{'layui-this':registerMsg.userExtendedInfo.country_text == item.content}">{{item.content}}
                        </dd>
                    </dl>
                </div>
            </div>
            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;证件类型:</label>
                <div class="layui-unselect col-xs-3 layui-form-select  min_wit">
                    <div class="layui-select-title">
                        <input type="text" class="layui-input layui-unselect" readonly="readonly" value=""
                               placeholder="" ng-model="registerMsg.userExtendedInfo.idType_text"/>
                        <i class="layui-edge"></i>
                    </div>
                    <dl class="layui-anim layui-anim-upbit">
                        <dd ng-repeat="item in allCard"
                            ng-click="setCardType(0, item)"
                            ng-class="{'layui-this':registerMsg.userExtendedInfo.idType_text == item.content}">{{item.content}}
                        </dd>
                    </dl>
                </div>
            </div>
            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;证件号:</label>
                <input class="form_control col-xs-3 min_wit" ng-model="registerMsg.user.idCard" type="text" maxlength="18" ng-change="checkCardNum(1)"/>
                <em style="color: red;line-height: 40px;margin-left: 15px;display: inline-flex;" ng-if="idCard1Invalid">学生身份证号有误</em>
                <em style="color: red;line-height: 40px;margin-left: 25px;display: inline-flex;" ng-if="isStu">用于登录，请确保正确填写。</em>
            </div>
            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;户口所在地:</label>
                <input class="form_control col-xs-1 min_wit" type="text" value="{{registerMsg.user.permanentAddress0}}" readonly/>
                <input class="form_control col-xs-2 min_wit" ng-model="registerMsg.user.permanentAddress" type="text" placeholder="街道+门牌号"/>
                <em style="color: red;line-height: 40px;margin-left: 15px;display: inline-flex;" ng-if="isStu">如：东正街6号、北二巷单号1号、广洲路七巷双号84号、步行街5栋、金塔村4组。请不要输入空格等特殊符号。</em>
            </div>
            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;出生所在地:</label>
                <input class="form_control col-xs-3 min_wit" ng-model="registerMsg.userExtendedInfo.homeplace" type="text"/>
            </div>
            <!-- 身份证号联动 --><!-- 日期插件 -->
            <!-- <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;出生日期:</label>
                <input class="form_control col-xs-3" ng-model="selRow.user_birthday" type="text"/>
                <div class="form_group col-xs-3 right" style="padding: 0">
                    <input class="date-time-picker form_control" ng-model="selRow.birth_day"
                           type="text"
                           plugintype="datetime">
                </div>
            </div> -->
            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;健康状况:</label>
                <div class="layui-unselect col-xs-3 layui-form-select  min_wit">
                    <div class="layui-select-title">
                        <input type="text" class="layui-input layui-unselect" readonly="readonly" value=""
                               placeholder="" ng-model="registerMsg.userExtendedInfo.healthCondition_text"/>
                        <i class="layui-edge"></i>
                    </div>
                    <dl class="layui-anim layui-anim-upbit">
                        <dd ng-repeat="item in allHealth"
                            ng-click="registerMsg.userExtendedInfo.healthCondition_text = item.label;registerMsg.userExtendedInfo.healthCondition = item.value"
                            ng-class="{'layui-this':registerMsg.userExtendedInfo.healthCondition_text == item.label}">{{item.label}}
                        </dd>
                    </dl>
                </div>
            </div>
            <div class="form_row border_bottom" ng-show="false">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;招生类型:</label>
                <div class="layui-unselect col-xs-3 layui-form-select  min_wit">
                    <div class="layui-select-title">
                        <input type="text" class="layui-input layui-unselect" readonly="readonly" value=""
                               placeholder="" ng-model="registerMsg.user.type_text"/>
                        <i class="layui-edge"></i>
                    </div>
                    <dl class="layui-anim layui-anim-upbit">
                        <dd ng-repeat="item in enrollType"
                            ng-click="registerMsg.user.type_text = item.label;registerMsg.user.type = item.value"
                            ng-class="{'layui-this':registerMsg.user.type_text == item.label}">{{item.label}}
                        </dd>
                    </dl>
                </div>
            </div>
            <!--填写毕业小学信息-->
            
        </div>
        <div class="border_left border_right border_top" style="float: left;width: 100%">
            <div class="form_row border_bottom">
                <label class="form_label title col-xs-12">学生个人扩展信息</label>
            </div>
            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;是否是独生子女:</label>
                <div class="layui-unselect col-xs-3 layui-form-select  min_wit">
                    <div class="layui-select-title">
                        <input type="text" class="layui-input layui-unselect" readonly="readonly" value=""
                               placeholder="" ng-model="registerMsg.userExtendedInfo.oneChild_text"/>
                        <i class="layui-edge"></i>
                    </div>
                    <dl class="layui-anim layui-anim-upbit">
                        <dd ng-repeat="item in toChoose"
                            ng-click="registerMsg.userExtendedInfo.oneChild_text = item.label;registerMsg.userExtendedInfo.oneChild = item.value"
                            ng-class="{'layui-this':registerMsg.userExtendedInfo.oneChild_text == item.label}">{{item.label}}
                        </dd>
                    </dl>
                </div>
            </div>
            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight" ng-show="'primary' == toSignUp"><em>*</em>&nbsp;是否上过幼儿园:</label>
                <label class="form_label col-xs-2 border_right text_right weight" ng-show="'junior' == toSignUp"><em>*</em>&nbsp;是否上过小学:</label>
                <div class="layui-unselect col-xs-3 layui-form-select  min_wit">
                    <div class="layui-select-title">
                        <input type="text" class="layui-input layui-unselect" readonly="readonly" value=""
                               placeholder="" ng-model="registerMsg.userExtendedInfo.graduated_text"/>
                        <i class="layui-edge"></i>
                    </div>
                    <dl class="layui-anim layui-anim-upbit">
                        <dd ng-repeat="item in toChoose"
                            ng-click="registerMsg.userExtendedInfo.graduated_text = item.label;registerMsg.userExtendedInfo.graduated = item.value"
                            ng-class="{'layui-this':registerMsg.userExtendedInfo.graduated_text == item.label}">{{item.label}}
                        </dd>
                    </dl>
                </div>
            </div>
            <div class="form_row border_bottom" ng-show="'primary' == toSignUp && registerMsg.userExtendedInfo.graduated">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;幼儿园名称:</label>
                <input class="form_control col-xs-3 min_wit" ng-model="registerMsg.userExtendedInfo.formerSchool" type="text"/>
            </div>
            <div class="form_row border_bottom" ng-show="'junior' == toSignUp && registerMsg.userExtendedInfo.graduated">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;小学名称:</label>
                <input class="form_control col-xs-3 min_wit" ng-model="registerMsg.userExtendedInfo.formerSchool" type="text" ng-blur="checkStuCode()"/>
            </div>
            <div class="form_row border_bottom" ng-show="'junior' == toSignUp && registerMsg.userExtendedInfo.graduated">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;学籍号:</label>
                <input class="form_control col-xs-3 min_wit" ng-model="registerMsg.userExtendedInfo.schoolNumber" type="text" ng-blur="checkStuCode()"/>
                <em style="color: red;line-height: 40px;margin-left: 15px;display: inline-flex;" ng-show="!stu_code_pass && registerMsg.userExtendedInfo.schoolNumber">学籍号与学生和学校不匹配</em>
            </div>
            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;是否是留守儿童:</label>
                <div class="layui-unselect col-xs-3 layui-form-select  min_wit">
                    <div class="layui-select-title">
                        <input type="text" class="layui-input layui-unselect" readonly="readonly" value=""
                               placeholder="" ng-model="registerMsg.userExtendedInfo.leftBehindChildren_text"/>
                        <i class="layui-edge"></i>
                    </div>
                    <dl class="layui-anim layui-anim-upbit">
                        <dd ng-repeat="item in toChoose"
                            ng-click="registerMsg.userExtendedInfo.leftBehindChildren_text = item.label;registerMsg.userExtendedInfo.leftBehindChildren = item.value"
                            ng-class="{'layui-this':registerMsg.userExtendedInfo.leftBehindChildren_text == item.label}">{{item.label}}
                        </dd>
                    </dl>
                </div>
            </div>
            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;是否是进城务工子女:</label>
                <div class="layui-unselect col-xs-3 layui-form-select  min_wit">
                    <div class="layui-select-title">
                        <input type="text" class="layui-input layui-unselect" readonly="readonly" value=""
                               placeholder="" ng-model="registerMsg.userExtendedInfo.workToCity_text"/>
                        <i class="layui-edge"></i>
                    </div>
                    <dl class="layui-anim layui-anim-upbit">
                        <dd ng-repeat="item in toChoose"
                            ng-click="registerMsg.userExtendedInfo.workToCity_text = item.label;registerMsg.userExtendedInfo.workToCity = item.value"
                            ng-class="{'layui-this':registerMsg.userExtendedInfo.workToCity_text == item.label}">{{item.label}}
                        </dd>
                    </dl>
                </div>
            </div>
            <!-- 孤儿不要监护人？孤儿不要监护人2？？ -->
            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;是否孤儿:</label>
                <div class="layui-unselect col-xs-3 layui-form-select  min_wit">
                    <div class="layui-select-title">
                        <input type="text" class="layui-input layui-unselect" readonly="readonly" value=""
                               placeholder="" ng-model="registerMsg.userExtendedInfo.orphan_text"/>
                        <i class="layui-edge"></i>
                    </div>
                    <dl class="layui-anim layui-anim-upbit">
                        <dd ng-repeat="item in toChoose"
                            ng-click="registerMsg.userExtendedInfo.orphan_text = item.label;registerMsg.userExtendedInfo.orphan = item.value"
                            ng-class="{'layui-this':registerMsg.userExtendedInfo.orphan_text == item.label}">{{item.label}}
                        </dd>
                    </dl>
                </div>
            </div>
            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;是否残疾:</label>
                <div class="layui-unselect col-xs-3 layui-form-select  min_wit">
                    <div class="layui-select-title">
                        <input type="text" class="layui-input layui-unselect" readonly="readonly" value=""
                               placeholder="" ng-model="registerMsg.userExtendedInfo.disabilityType_text"/>
                        <i class="layui-edge"></i>
                    </div>
                    <dl class="layui-anim layui-anim-upbit">
                        <dd ng-repeat="item in [{l:'是',v:'1'},{l:'否',v:'0'}]"
                            ng-click="registerMsg.userExtendedInfo.disabilityType = item.v;registerMsg.userExtendedInfo.disabilityType_text = item.l"
                            ng-class="{'layui-this':registerMsg.userExtendedInfo.disabilityType_text == item.l}">{{item.l}}
                        </dd>
                    </dl>
                </div>
            </div>
        </div>
        <!-- 法定监护人1 -->
        <div class="border_left border_right border_top" style="float: left;width: 100%">
            <div class="form_row border_bottom">
                <label class="form_label title col-xs-12">法定监护人信息一</label>
            </div>
            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;姓名:</label>
                <input class="form_control col-xs-3 min_wit" ng-model="registerMsg.userGuardianInfos[0].name" type="text"/>
            </div>
            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;与学生关系:</label>
                <div class="layui-unselect col-xs-3 layui-form-select min_wit">
                   <div class="layui-select-title">
                       <input type="text" class="layui-input layui-unselect" readonly="readonly" value=""
                              placeholder="" ng-model="registerMsg.userGuardianInfos[0].userRelationship_text"/>
                       <i class="layui-edge"></i>
                   </div>
                   <dl class="layui-anim layui-anim-upbit">
                       <dd ng-repeat="item in student_relationList" ng-click="set_student_relation(1, item)"
                           ng-class="{'layui-this':registerMsg.userGuardianInfos[0].userRelationship_text == item.content}">{{item.content}}
                       </dd>
                   </dl>
               </div>
                <em style="color: red;line-height: 40px;margin-left: 15px;display: inline-flex;" ng-if="isStu">优先选择父亲。</em>
            </div>
            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;工作单位:</label>
                <input class="form_control col-xs-3 min_wit" ng-model="registerMsg.userGuardianInfos[0].workUnit" type="text"/>
            </div>
            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;户口所在地:</label>
                <input class="form_control col-xs-3 min_wit" ng-model="registerMsg.userGuardianInfos[0].accountAddress" type="text"/>
            </div>
            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;联系电话:</label>
                <!-- 后期加校验 -->
                <input class="form_control col-xs-3 min_wit" ng-model="registerMsg.userGuardianInfos[0].phone" type="text" maxlength="11" ng-change="checkPhone(2)"/>
                <em style="color: red;line-height: 40px;margin-left: 15px;display: inline-flex;" ng-if="phone2Invalid">监护人一联系电话格式有误</em>
            </div>
            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;是否为法定监护人:</label>
                <div class="layui-unselect col-xs-3 layui-form-select  min_wit">
                    <div class="layui-select-title">
                        <input type="text" class="layui-input layui-unselect" readonly="readonly" value=""
                               placeholder="" ng-model="registerMsg.userGuardianInfos[0].guarder_text"/>
                        <i class="layui-edge"></i>
                    </div>
                    <dl class="layui-anim layui-anim-upbit">
                        <dd ng-repeat="item in toChoose"
                            ng-click="registerMsg.userGuardianInfos[0].guarder_text = item.label;registerMsg.userGuardianInfos[0].guarder = item.value"
                            ng-class="{'layui-this':registerMsg.userGuardianInfos[0].guarder_text == item.label}">{{item.label}}
                        </dd>
                    </dl>
                </div>
            </div>
            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;证件类型:</label>
                <div class="layui-unselect col-xs-3 layui-form-select  min_wit">
                    <div class="layui-select-title">
                        <input type="text" class="layui-input layui-unselect" readonly="readonly" value=""
                               placeholder="" ng-model="registerMsg.userGuardianInfos[0].idType_text"/>
                        <i class="layui-edge"></i>
                    </div>
                    <dl class="layui-anim layui-anim-upbit">
                        <dd ng-repeat="item in allCard"
                            ng-click="setCardType(1, item)"
                            ng-class="{'layui-this':registerMsg.userGuardianInfos[0].idType_text == item.content}">{{item.content}}
                        </dd>
                    </dl>
                </div>
            </div>
            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;证件号:</label>
                <input class="form_control col-xs-3 min_wit" ng-model="registerMsg.userGuardianInfos[0].idNumber" maxlength="18" type="text" ng-change="checkCardNum(2)"/>
                <em style="color: red;line-height: 40px;margin-left: 15px;display: inline-flex;" ng-if="idCard2Invalid">监护人一身份证号有误</em>
            </div>
            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;职务:</label>
                <input class="form_control col-xs-3 min_wit" ng-model="registerMsg.userGuardianInfos[0].duties" type="text"/>
            </div>
            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;学历:</label>
                <input class="form_control col-xs-3 min_wit" ng-model="registerMsg.userGuardianInfos[0].education" type="text"/>
            </div>
        </div>
        
        <!-- 法定监护人2 -->
        <div class="border_left border_right border_top" style="float: left;width: 100%">
            <div class="form_row border_bottom">
                <label class="form_label title col-xs-12">法定监护人信息二</label>
            </div>
            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;姓名:</label>
                <input class="form_control col-xs-3 min_wit" ng-model="registerMsg.userGuardianInfos[1].name" type="text"/>
            </div>
            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;与学生关系:</label>
                <div class="layui-unselect col-xs-3 layui-form-select min_wit">
                   <div class="layui-select-title">
