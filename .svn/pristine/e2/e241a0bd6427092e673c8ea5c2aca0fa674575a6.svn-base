            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;注册手机号确认:</label>
                <span class="col-xs-3" ng-bind="registerMsg.user.phone" style="text-align: left;"></span>
                <button class="btn btn-primary sms_btn" id="send"
                        value="smsCodeValue" ng-click="senSmsCode()" ng-disabled="senBtnDisable">{{smsCodeValue}}
                </button>
            </div>
            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;验证码:</label>
                <input class="form_control col-xs-3" ng-model="registerMsg.user.smsCode" maxlength="6" type="text" placeholder="手机验证码" ng-change="checkSmsCode()"/>
                <em style="color: red;line-height: 40px;margin-left: 15px;display: inline-flex;" ng-if="smsCodeInvalid && 0 < registerMsg.user.smsCode.length">验证码错误</em>
            </div>
            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;密码:</label>
                <input class="form_control col-xs-3" ng-model="registerMsg.user.pwd" type="password" ng-change="checkPassword()"/>
                <em style="color: red;line-height: 40px;margin-left: 15px;display: inline-flex;" ng-if="passwordInvalid">请输入6-16位的字母和数字组合</em>
            </div>
            <div class="form_row border_bottom">
                <label class="form_label col-xs-2 border_right text_right weight"><em>*</em>&nbsp;确认密码:</label>
                <input class="form_control col-xs-3" ng-model="registerMsg.user.repwd" type="password" ng-change="checkRePassword()"/>
                <em style="color: red;line-height: 40px;margin-left: 15px;display: inline-flex;" ng-if="repasswordInvalid">两次输入的密码不相同</em>
            </div>
        </div>

